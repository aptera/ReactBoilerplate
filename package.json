{
  "name": "babelwebpack",
  "version": "1.0.0",
  "description": "boilerplate babel environment with webpack",
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "analyze": "webpack --config webpack/analyze.config.js",
    "test": "jest --notify --watch --coverage",
    "json": "json-server --watch src/data/generate.js"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>=/__mocks__/fileMock.js",
      "\\.(scss|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "author": "David Federspiel",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptera/ReactBoilerplate.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap-loader": "^2.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "json-server": "^0.12.1",
    "lodash": "^4.17.4",
    "merge": "^1.2.0",
    "node-sass": "^4.7.1",
    "npm-watch": "^0.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-routes": "^0.2.6",
    "react-test-renderer": "^15.6.2",
    "react-transition-group": "^2.2.1",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "object.values": "^1.0.4",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "sec-to-min": "^1.0.1"
  }
}
